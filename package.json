{
  "name": "sound-fusion",
  "productName": "SoundFusion",
  "author": "Teemu Taskula",
  "version": "1.0.0",
  "description": "A music player app that combines tracks from multiple sources",
  "private": true,
  "main": "./src/electron-starter.js",
  "homepage": "./",
  "devDependencies": {
    "electron": "1.6.10",
    "electron-builder": "19.16.0",
    "react-scripts": "1.0.7"
  },
  "dependencies": {
    "color": "1.0.3",
    "immutability-helper": "2.2.3",
    "pouchdb": "6.2.0",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "redux-actions": "2.2.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.4",
    "styled-components": "2.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "electron-dev": "ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-dev:win": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "prepackage": "npm run build",
    "package": "build"
  },
  "build": {
    "appId": "com.Taskula.SoundFusion",
    "files": "!build-resources${*/}",
    "directories": {
      "buildResources": "./build-resources"
    },
    "mac": {
      "category": "public.app-category.music"
    }
  },
  "license": "ISC"
}
